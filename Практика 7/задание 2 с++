#include <iostream>
#include <set>
#include <algorithm>
using namespace std;
#include <iterator>

int main()
{
    set<int> A;
    set<int> B;
    set<int> result;

    int x;

    cout << "Введите 5 элементов множества A: ";
    for (int i = 0; i < 5; i++){
        cin >> x;
        A.insert(x);
    }

    cout << "Введите 5 элементов множества B: ";
    for (int i = 0; i < 5; i++){
        cin >> x;
        B.insert(x);
    }

    set_difference(B.begin(), B.end(),A.begin(), A.end(),inserter(result, result.begin()));

    if (result.size() > 0) {

    cout << "Наименьший элемент, который есть в B, но нет в A: " << *result.begin() << endl;
    } else {
    cout << "Все элементы B есть в A" << endl;
    }
}
